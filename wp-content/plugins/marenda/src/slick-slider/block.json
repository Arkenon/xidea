{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "marenda/slick-slider",
  "version": "0.1.0",
  "title": "Slick Slider",
  "category": "widgets",
  "icon": "admin-page",
  "description": "Slick slider block converts core columns block into slick slider. Preview is avaliable only front-end. It is created for Marenda Theme.",
  "supports": {
	"html": false
  },
  "attributes": {
	"sliderId": {
	  "type": "string",
	  "default": "my-slider-1"
	},
	"slidesToShow": {
	  "type": "number",
	  "default": 2
	},
	"slidesToScroll": {
	  "type": "number",
	  "default": 1
	}
  },
  "textdomain": "marenda",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
