{
	"$schema": "https://schemas.wp.org/wp/6.0/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"gradients": [
				{
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--light-green) 100%)",
					"name": "Vertical secondary to tertiary to bottom",
					"slug": "vertical-secondary-to-light-green-to-bottom"
				},
				{
					"gradient": "linear-gradient(to top,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--light-green) 100%)",
					"name": "Vertical secondary to tertiary to top",
					"slug": "vertical-secondary-to-light-green-to-top"
				},
				{
					"gradient": "linear-gradient(to right,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--light-green) 100%)",
					"name": "Horizontal secondary to tertiary to right",
					"slug": "horizontal-secondary-to-light-green-to-right"
				},
				{
					"gradient": "linear-gradient(to left,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--light-green) 100%)",
					"name": "Horizontal secondary to tertiary to left",
					"slug": "horizontal-secondary-to-light-green-to-left"
				},
				{
					"gradient": "linear-gradient(to top,var(--wp--preset--color--light-gray) 0%,var(--wp--preset--color--light-green) 100%)",
					"name": "Vertical ligth gray to light green to top",
					"slug": "vertical-light-gray-to-light-green-to-top"
				}
			],
			"palette": [
				{
					"color": "#000000",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#3e4fcc",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#c49f52",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#ebedfa",
					"name": "Primary Light",
					"slug": "primary-light"
				},
				{
					"color": "#003661",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"layout": {
			"contentSize": "1670px",
			"wideSize": "1670px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'Montserrat', sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				},
				{
					"fontFamily": "'Tiro Gurmukhi', serif",
					"name": "Tiro Gurmukhi",
					"slug": "tiro-gurmukhi"
				}
			],
			"fontSizes": [
				{
					"size": "5.375rem",
					"slug": "fs-1"
				},
				{
					"size": "4.5rem",
					"slug": "fs-2"
				},
				{
					"size": "3.75rem",
					"slug": "fs-3"
				},
				{
					"size": "3rem",
					"slug": "fs-4"
				},
				{
					"size": "2.25rem",
					"slug": "fs-5"
				},
				{
					"size": "1.5rem",
					"slug": "fs-6"
				},
				{
					"size": "1.25rem",
					"slug": "fs-7"
				},
				{
					"size": "1.125rem",
					"slug": "fs-8"
				},
				{
					"size": "1rem",
					"slug": "fs-9"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--fs-8)"
				}
			},
			"core/paragraph": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "var(--wp--preset--font-size--fs-7)",
					"fontWeight": "400",
					"lineHeight": "150%"
				}
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
					"fontSize": "var(--wp--preset--font-size--fs-2)",
					"fontWeight": "400",
					"lineHeight": "110%"
				}
			},
			"core/query-title": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
					"fontSize": "var(--wp--preset--font-size--fs-3)",
					"fontWeight": "400",
					"lineHeight": "110%"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--tertiary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
					"fontSize": "var(--wp--preset--font-size--fs-1)",
					"fontWeight": "700",
					"lineHeight": "110%"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
					"fontSize": "var(--wp--preset--font-size--fs-2)",
					"fontWeight": "400",
					"lineHeight": "120%"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
					"fontSize": "var(--wp--preset--font-size--fs-3)",
					"fontWeight": "400",
					"lineHeight": "120%"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
					"fontSize": "var(--wp--preset--font-size--fs-4)",
					"fontWeight": "400",
					"lineHeight": "120%"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
					"fontSize": "var(--wp--preset--font-size--fs-5)",
					"fontWeight": "400",
					"lineHeight": "110%"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
					"fontSize": "var(--wp--preset--font-size--fs-6)",
					"fontWeight": "400",
					"lineHeight": "110%"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--tiro-gurmukhi)",
			"fontSize": "var(--wp--preset--font-size--fs-7)",
			"lineHeight": "150%"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}